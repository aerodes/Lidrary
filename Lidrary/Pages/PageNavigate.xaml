<Page x:Class="Lidrary.Pages.PageNavigate"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Lidrary.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageNavigate">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" 
                    VerticalAlignment="Center">
            <Button Name="btnGoToInsert" Content="Вкладыши" Style="{StaticResource btnStyle}" Click="btnGoToInsert_Click"/>
            <Button Name="btnGoToBooks" Content="Книги" Style="{StaticResource btnStyle}"/>
            <Button Name="btnGoToReaders" Content="Читатели" Style="{StaticResource btnStyle}"/>
            <Button Name="btnGoToAutors" Content="Авторы" Style="{StaticResource btnStyle}"/>
        </StackPanel>
        
    </Grid>
</Page>
